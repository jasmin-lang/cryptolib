  (u64)[output + 8*0] = kk[0];

  for i=2 to 8
  {
    kk[i] = (64u)((i == 7) ? k15 : k[2*i+1]);
    kk[i] <<= 32;
    aux = (64u)((i == 7) ? k14 : k[2*i]);
    kk[i] ^= aux;
    (u64)[output + 8*(i-1)] = kk[i-1];
  }

  (u64)[output + 8*7] = kk[7];

  //len = s_len;
  tt = s_len;
  _, _, _, _, _, len = #protect(tt, ms);

  output, len = __update_ptr_ref1pv1(output, len, 64);

  s_output = output;
  s_len = len;

  return s_output, s_len;
}

