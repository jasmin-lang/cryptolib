// speculative constant type checker: Current state is Trans ({}, (! (j <u len))), use #set_msf before
inline fn __store_last_ref1pv1(
  #transient stack u64 s_output s_len,
  #secret reg u32[16] k,
  #secret stack u32 k14 k15,
  #msf reg u64 ms)
  ->
  #msf reg u64
{
  #public inline int i;
  #public reg u64 j output len len8;
  #secret reg u64 t;
  #secret reg u32 u;
  #secret stack u32[16] s_k;
  #secret reg u8 pi;
  #transient reg u64 tt;
  #public reg bool b;

  // copy k to stack
  for i=0 to 14
  { s_k[i] = k[i]; }

  u = k14;
  s_k[14] = u;

  u = k15;
  s_k[15] = u;

  //output = s_output;
  tt = s_output;
  _, _, _, _, _, output = #protect(tt, ms);

  //len = s_len;
  tt = s_len;
  _, _, _, _, _, len = #protect(tt, ms);


  // u64 at a time
  len8 = len;
  len8 >>= 3;
  j = 0;
  while {
    b = (j < len8);
  }(b)
  {
    _, ms = #set_msf(b, ms);
    t = s_k[u64 (int)j];
    (u64)[output + 8*j] = t;
    j += 1;
  }
  _, ms = #set_msf(!b, ms);
  j <<= 3;

  // u8 at a time
  while {
    b = (j < len);
  }(b)
  {
    _, ms = #set_msf(b, ms);
    pi = s_k[u8 (int)j];
    (u8)[output + j] = pi;
    j += 1;
  }
  _, ms = #set_msf(!b, ms);

  return ms;
}
