param int CHACHA_ROUNDS=20;

from Jade require "crypto_stream/chacha/common/amd64/ref2/chacha.jinc"

export fn jade_stream_chacha_chacha20_amd64_ref2_xor(reg u64 output plain len nonce key) -> reg u64
{
  reg u64 r;
  __chacha_xor_ref2(output, plain, len, nonce, key);
  _,_,_,_,_,r = #set0();
  return r;
}


export fn jade_stream_chacha_chacha20_amd64_ref2(reg u64 output len nonce key) -> reg u64
{
  reg u64 r;
  __chacha_ref2(output, len, nonce, key);
  _,_,_,_,_,r = #set0();
  return r;
}


